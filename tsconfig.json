{
  "compilerOptions": {
    "target": "es2015",
    "lib": ["es2015", "es2017", "dom"],
    "moduleResolution": "node",
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "zhiman-design": ["src"],
      "zhiman-design/*": ["src/*", "*"]
    }
  },
  "include": [".dumirc.ts", "src/**/*"]
}
